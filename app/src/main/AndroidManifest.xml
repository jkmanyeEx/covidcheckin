<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.schoolvote.covidcheckin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logobig"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logobig"
        android:supportsRtl="true"
        android:theme="@style/Theme.Covidcheckin"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".AboutActivity"
            android:label="@string/about"/>
        <activity
            android:name=".MainMenuActivity"
            android:label="@string/menu" />
        <activity
            android:name=".RegisterActivity"
            android:label="@string/register" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/login" />
        <activity
            android:name=".ReserveActivity"
            android:label="@string/reserve" />
        <activity
            android:name=".SelfCheckActivity"
            android:label="@string/selfcheck" />
        <activity
            android:name=".CheckoutActivity"
            android:label="@string/checkout" />
        <activity
            android:name=".CheckinActivity"
            android:label="@string/checkin" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>